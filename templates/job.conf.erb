
Job {
<% if name                  %>  name                    = "<%= name %>"<% end %>
<% if client                %>  client                  = "<%= client %>-fd"<% end %>
<% if type                  %>  type                    = "<%= type %>"<% end %>
<% if working_dir           %>  write bootstrap         = "<%= working_dir %>/<%= name %>.bsr"<% end %>
<% if messages              %>  messages                = "<%= messages %>"<% end %>
<% if jobdefs               %>  jobdefs                 = "<%= jobdefs %>"<% end %>

<% if fileset               %>  fileset                 = "<%= fileset %>"<% end %>
<% if storage               %>  storage                 = "<%= storage %>"<% end %>
<% if bschedule             %>  schedule                = "<%= bschedule %>"<% end %>

<% if pool                  %>  pool                    = "<%= pool %>"<% end %>
<% if full_pool             %>  full backup pool        = "<%= full_pool %>"<% end %>
<% if incremental_pool      %>  incremental backup pool = "<%= incremental_pool %>"<% end %>

<% if client_run_before_job %>  client run before job   = "<%= client_run_before_job %>"<% end %>
<% if client_run_after_job  %>  client run after job    = "<%= client_run_after_job %>"<% end %>
<% if server_run_before_job %>  run before job          = "<%= server_run_before_job %>"<% end %>
<% if server_run_after_job  %>  run after job           = "<%= server_run_after_job %>"<% end %>
}
