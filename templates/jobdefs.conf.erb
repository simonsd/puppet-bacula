### File managed with puppet ###
### Served by:        '<%= scope.lookupvar('::servername') %>'
### Module:           '<%= scope.to_hash['module_name'] %>'
### Template source:  '<%= template_source %>'

JobDefs {
  name     = "<%= @name %>"
  type     = "<%= @jobtype %>"
  level    = "<%= @level %>"
  fileset  = "<%= @fileset %>"
  schedule = "<%= @bschedule %>"
<% if @messages -%>
  messages = "<%= @messages %>"
<% end -%>
  pool     = "<%= @pool %>"
  priority = "<%= @priority %>"
}
